	<queryString language="SQL">
		<![CDATA[SELECT
  s.nombre AS nombre_seccion,
  p.nombre AS nombre_producto,
  p.stock AS stock,
  p.precio AS precio_sin_iva,
  ROUND(p.precio * 1.21, 2) AS precio_con_iva
FROM
  producto p
JOIN
  seccion s ON p.seccion = s.codigo
ORDER BY
  s.codigo, p.nombre]]>
	</queryString>
